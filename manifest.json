{
    "manifest_version": 2,
    "name": "Copy Link",
    "version": "1.0",
    "description": "Copy the Link from current tab",
    "author": "Dhanya Abhirami",
    "icons": {
        "48": "icons/48x48.png"
    },
    "browser_action" : {
        "default_icon": "icons/48x48.png",
        "default_popup": "html/popup.html",
        "default_title": "Copy Link"
    },
    "background":{
        "scripts":["js/background.js"]
    },
    "permissions": ["tabs"]
  }