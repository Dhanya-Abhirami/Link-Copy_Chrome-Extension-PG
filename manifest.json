{
    "manifest_version": 2,
    "name": "Copy URL",
    "version": "1.0",
    "description": "Copy the URL from current tab",
    "author": "Dhanya Abhirami",
    "icons": {
        "48": "icons/48x48.png"
    },
    "browser_action" : {
        "default_icon": "icons/48x48.png",
        "default_popup": "popup.html",
        "default_title": "Copy URL"
    },
    "background":{
        "scripts":["background.js"]
    },
    "permissions": ["tabs"]
  }